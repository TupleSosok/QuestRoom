CFLAGS_station= -std=c99 -Wall -g -Os -mmcu=${STATION_BOARD} -DF_CPU=${F_CPU} \
		-DENCODER_1_DT=P${ENCODER_1_DT} -DPORT_ENCODER_1_DT=${PORT_ENCODER_1_DT} -DDDR_ENCODER_1_DT=${DDR_ENCODER_1_DT} -DPIN_ENCODER_1_DT=${PIN_ENCODER_1_DT}\
		-DENCODER_1_CLK=P${ENCODER_1_CLK} -DPORT_ENCODER_1_CLK=${PORT_ENCODER_1_CLK} -DDDR_ENCODER_1_CLK=${DDR_ENCODER_1_CLK} -DPIN_ENCODER_1_CLK=${PIN_ENCODER_1_CLK}\
		-DENCODER_2_DT=P${ENCODER_2_DT} -DPORT_ENCODER_2_DT=${PORT_ENCODER_2_DT} -DDDR_ENCODER_2_DT=${DDR_ENCODER_2_DT} -DPIN_ENCODER_2_DT=${PIN_ENCODER_2_DT}\
		-DENCODER_2_CLK=P${ENCODER_2_CLK} -DPORT_ENCODER_2_CLK=${PORT_ENCODER_2_CLK} -DDDR_ENCODER_2_CLK=${DDR_ENCODER_2_CLK} -DPIN_ENCODER_2_CLK=${PIN_ENCODER_2_CLK}\
		-DENCODER_3_DT=P${ENCODER_3_DT} -DPORT_ENCODER_3_DT=${PORT_ENCODER_3_DT} -DDDR_ENCODER_3_DT=${DDR_ENCODER_3_DT} -DPIN_ENCODER_3_DT=${PIN_ENCODER_3_DT}\
		-DENCODER_3_CLK=P${ENCODER_3_CLK} -DPORT_ENCODER_3_CLK=${PORT_ENCODER_3_CLK} -DDDR_ENCODER_3_CLK=${DDR_ENCODER_3_CLK} -DPIN_ENCODER_3_CLK=${PIN_ENCODER_3_CLK}\
		-DSPEAKER=P${SPEAKER} -DPORT_SPEAKER=${PORT_SPEAKER} -DDDR_SPEAKER=${DDR_SPEAKER} -DPIN_SPEAKER=${PIN_SPEAKER}\
		-DSTEPPER_PA=P${STEPPER_PA} -DPORT_STEPPER_PA=${PORT_STEPPER_PA} -DDDR_STEPPER_PA=${DDR_STEPPER_PA} -DPIN_STEPPER_PA=${PIN_STEPPER_PA}\
		-DSTEPPER_PB=P${STEPPER_PB} -DPORT_STEPPER_PB=${PORT_STEPPER_PB} -DDDR_STEPPER_PB=${DDR_STEPPER_PB} -DPIN_STEPPER_PB=${PIN_STEPPER_PB}\
		-DSTEPPER_NA=P${STEPPER_NA} -DPORT_STEPPER_NA=${PORT_STEPPER_NA} -DDDR_STEPPER_NA=${DDR_STEPPER_NA}	-DPIN_STEPPER_NA=${PIN_STEPPER_NA}\
		-DSTEPPER_NB=P${STEPPER_NB} -DPORT_STEPPER_NB=${PORT_STEPPER_NB} -DDDR_STEPPER_NB=${DDR_STEPPER_NB} -DPIN_STEPPER_NB=${PIN_STEPPER_NB}\
		-DTUMBLER_1=P${TUMBLER_1} -DPORT_TUMBLER_1=${PORT_TUMBLER_1} -DDDR_TUMBLER_1=${DDR_TUMBLER_1} -DPIN_TUMBLER_1=${PIN_TUMBLER_1}\
		-DTUMBLER_2=P${TUMBLER_2} -DPORT_TUMBLER_2=${PORT_TUMBLER_2} -DDDR_TUMBLER_2=${DDR_TUMBLER_2} -DPIN_TUMBLER_2=${PIN_TUMBLER_2}\
		-DTUMBLER_3=P${TUMBLER_3} -DPORT_TUMBLER_3=${PORT_TUMBLER_3} -DDDR_TUMBLER_3=${DDR_TUMBLER_3} -DPIN_TUMBLER_3=${PIN_TUMBLER_3}\
		-DLED_1_R=P${LED_1_R} -DPORT_LED_1_R=${PORT_LED_1_R} -DDDR_LED_1_R=${DDR_LED_1_R} -DPIN_LED_1_R=${PIN_LED_1_R}\
		-DLED_1_Y=P${LED_1_Y} -DPORT_LED_1_Y=${PORT_LED_1_Y} -DDDR_LED_1_Y=${DDR_LED_1_Y} -DPIN_LED_1_Y=${PIN_LED_1_Y}\
		-DLED_1_G=P${LED_1_G} -DPORT_LED_1_G=${PORT_LED_1_G} -DDDR_LED_1_G=${DDR_LED_1_G} -DPIN_LED_1_G=${PIN_LED_1_G}\
		-DLED_2_R=P${LED_2_R} -DPORT_LED_2_R=${PORT_LED_2_R} -DDDR_LED_2_R=${DDR_LED_2_R} -DPIN_LED_2_R=${PIN_LED_2_R}\
		-DLED_2_Y=P${LED_2_Y} -DPORT_LED_2_Y=${PORT_LED_2_Y} -DDDR_LED_2_Y=${DDR_LED_2_Y} -DPIN_LED_2_Y=${PIN_LED_2_Y}\
		-DLED_2_G=P${LED_2_G} -DPORT_LED_2_G=${PORT_LED_2_G} -DDDR_LED_2_G=${DDR_LED_2_G} -DPIN_LED_2_G=${PIN_LED_2_G}\
		-DLED_3_R=P${LED_3_R} -DPORT_LED_3_R=${PORT_LED_3_R} -DDDR_LED_3_R=${DDR_LED_3_R} -DPIN_LED_3_R=${PIN_LED_3_R}\
		-DLED_3_Y=P${LED_3_Y} -DPORT_LED_3_Y=${PORT_LED_3_Y} -DDDR_LED_3_Y=${DDR_LED_3_Y} -DPIN_LED_3_Y=${PIN_LED_3_Y}\
		-DLED_3_G=P${LED_3_G} -DPORT_LED_3_G=${PORT_LED_3_G} -DDDR_LED_3_G=${DDR_LED_3_G} -DPIN_LED_3_G=${PIN_LED_3_G}

		
TARGET_station=station
SRC_station=station.c


CFLAGS_mobile= -std=c99 -Wall -g -Os -mmcu=${MOBILE_BOARD} -DF_CPU=${F_CPU} \
		-DLCD_RS=P${LCD_RS} -DPORT_LCD_RS=${PORT_LCD_RS} -DDDR_LCD_RS=${DDR_LCD_RS} -DPIN_LCD_RS=${PIN_LCD_RS} \
		-DLCD_EN=P${LCD_EN} -DPORT_LCD_EN=${PORT_LCD_EN} -DDDR_LCD_EN=${DDR_LCD_EN} -DPIN_LCD_EN=${PIN_LCD_EN} \
		-DLCD_D4=P${LCD_D4} -DPORT_LCD_D4=${PORT_LCD_D4} -DDDR_LCD_D4=${DDR_LCD_D4} -DPIN_LCD_D4=${PIN_LCD_D4} \
		-DLCD_D5=P${LCD_D5} -DPORT_LCD_D5=${PORT_LCD_D5} -DDDR_LCD_D5=${DDR_LCD_D5} -DPIN_LCD_D5=${PIN_LCD_D5} \
		-DLCD_D6=P${LCD_D6} -DPORT_LCD_D6=${PORT_LCD_D6} -DDDR_LCD_D6=${DDR_LCD_D6} -DPIN_LCD_D6=${PIN_LCD_D6} \
		-DLCD_D7=P${LCD_D7} -DPORT_LCD_D7=${PORT_LCD_D7} -DDDR_LCD_D7=${DDR_LCD_D7} -DPIN_LCD_D7=${PIN_LCD_D7} \
		-DKEY_R1=P${KEY_R1} -DPORT_KEY_R1=${PORT_KEY_R1} -DDDR_KEY_R1=${DDR_KEY_R1} -DPIN_KEY_R1=${PIN_KEY_R1}\
		-DKEY_R2=P${KEY_R2} -DPORT_KEY_R2=${PORT_KEY_R2} -DDDR_KEY_R2=${DDR_KEY_R2} -DPIN_KEY_R2=${PIN_KEY_R2}\
		-DKEY_R3=P${KEY_R3} -DPORT_KEY_R3=${PORT_KEY_R3} -DDDR_KEY_R3=${DDR_KEY_R3} -DPIN_KEY_R3=${PIN_KEY_R3}\
		-DKEY_R4=P${KEY_R4} -DPORT_KEY_R4=${PORT_KEY_R4} -DDDR_KEY_R4=${DDR_KEY_R4} -DPIN_KEY_R4=${PIN_KEY_R4}\
		-DKEY_C1=P${KEY_C1} -DPORT_KEY_C1=${PORT_KEY_C1} -DDDR_KEY_C1=${DDR_KEY_C1} -DPIN_KEY_C1=${PIN_KEY_C1}\
		-DKEY_C2=P${KEY_C2} -DPORT_KEY_C2=${PORT_KEY_C2} -DDDR_KEY_C2=${DDR_KEY_C2} -DPIN_KEY_C2=${PIN_KEY_C2}\
		-DKEY_C3=P${KEY_C3} -DPORT_KEY_C3=${PORT_KEY_C3} -DDDR_KEY_C3=${DDR_KEY_C3} -DPIN_KEY_C3=${PIN_KEY_C3}

# -DLCD_SDA=P${LCD_SDA} -DPORT_LCD_SDA=${PORT_LCD_SDA} -DDDR_LCD_SDA=${DDR_LCD_SDA}\
		-DLCD_SCL=P${LCD_SCL} -DPORT_LCD_SCL=${PORT_LCD_SCL} -DDDR_LCD_SCL=${DDR_LCD_SCL}\
		
		
TARGET_mobile=mobile
SRC_mobile=mobile.c

all:
	${CC} ${CFLAGS_station} -o ${TARGET_station}.bin ${SRC_station}
	${OBJCOPY} -j .text -j .data -O ihex ${TARGET_station}.bin ${TARGET_station}.hex

	${CC} ${CFLAGS_mobile} -o ${TARGET_mobile}.bin ${SRC_mobile}
	${OBJCOPY} -j .text -j .data -O ihex ${TARGET_mobile}.bin ${TARGET_mobile}.hex

clean:
	rm -f *.bin *.hex